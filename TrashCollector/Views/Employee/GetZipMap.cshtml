
@model TrashCollector.Models.GetZipMapViewModel
@{
    ViewBag.Title = "Get Route";
}

<h2>@ViewBag.Title.</h2>
@using TrashCollector.Models

@using (Html.BeginForm("GetZipMap", "Employee", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    @Html.AntiForgeryToken()
    <h4>Gather List For:</h4>
    <hr />

    <div class="form-group">
        @Html.LabelFor(m => m.Zip, htmlAttributes: new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.Zip, htmlAttributes: new { @class = "form-control" })
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" class="btn btn-default" value="Submit" />
        </div>
    </div>

}

@section Scripts {
    ss    @Scripts.Render("~/bundles/jqueryval")
}
